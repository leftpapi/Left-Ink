oot=char[_X("\72\117\109\97\110\111\105\100\82\111\111\116\80\97\114\116")] local nearest,nilDist=nil,math.huge for _,m in ipairs(_W:GetDescendants())do if m:IsA(_X("\77\111\100\101\108")) and not m:IsDescendantOf(_gsvc(1)) and not m:IsDescendantOf(char) then local nm=m.Name:lower() if nm:find(_X("\103\117\97\114\100")) or nm:find(_X("\103\117\121")) or nm:find(_X("\115\113\117\105\100")) then local part=m:FindFirstChildWhichIsA(_X("\66\97\115\101\80\97\114\116")) if part then local dist=(part.Position-root.Position).Magnitude if dist<nilDist then nilDist=dist nearest=m end end end end end if nearest and _b then local targetPart=nearest:FindFirstChild(_X("\72\101\97\100")) or nearest:FindFirstChild(_X("\84\111\114\115\111")) if targetPart then local camPos=_C.CFrame.Position local dir=(targetPart.Position-camPos).Unit local cf=_C.CFrame local newCf=cf:Lerp(CFrame.new(camPos,targetPart.Position),_a) _C.CFrame=newCf end end end) _U.JumpRequest:Connect(function() if _d then local char=_P.Character if char then local hum=char:FindFirstChildOfClass(_X("\72\117\109\97\110\111\105\100")) if hum and hum:GetState()~=Enum.HumanoidStateType.Jumping then hum:ChangeState(Enum.HumanoidStateType.Jumping) end end end end) _showPrompt()