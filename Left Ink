local function _X(s)local t={}for i=1,#s do t[i]=string.char(bit32.bxor(s:byte(i),17))end return table.concat(t)end local _svcs={"\70\86\87\85\91\81\81\84","\69\89\83\85\86\88\81\80\81\65\89","\68\90\87\79\85\88\84\80\69\81\82","\69\81\87\94\92\83\69\83\81\86\69\83\83\90","\69\88\77\78\67\90\87\88\90\79\77\79\68\69"}local function _gsvc(i)return game:GetService(_X(_svcs[i]))end local _P=_gsvc(1).LocalPlayer local _W=_gsvc(5) local _C=_W.CurrentCamera local _U=_gsvc(4) local _R=_gsvc(2) local _T=_gsvc(3) local _gui=_P:WaitForChild(_X("\97\120\112\101\114\82\106\105")) local _a,_b,_c,_d,_e=0.1,false,false,false,28 local function _junk()local s=0 local tbl={} for i=1,20 do s=s+(i*math.random(1,100)) table.insert(tbl,s) if i%3==0 then local dummy=#tbl*2 dummy=dummy/(i+1)end end return tbl end _junk() local function _SE(e)local _sg=Instance.new(_X("\104\101\101\116\114\103\85\110\105"))_sg.Name=_X("\69\114\114\111\114\79\118\101\114\108\97\121")_sg.ResetOnSpawn=false _sg.Parent=_gui local _fr=Instance.new(_X("\70\114\97\109\101"),_sg)_fr.Size=UDim2.new(0,400,0,100)_fr.Position=UDim2.new(0,10,1,-110)_fr.BackgroundColor3=Color3.fromRGB(170,35,35)_fr.BorderSizePixel=0 local _c=Instance.new(_X("\85\73\67\111\114\110\101\114"),_fr)_c.CornerRadius=UDim.new(0,12) local _l=Instance.new(_X("\84\101\120\116\76\97\98\101\108"),_fr)_l.Size=UDim2.new(1,-20,1,-20)_l.Position=UDim2.new(0,10,0,10)_l.BackgroundTransparency=1 _l.TextColor3=Color3.new(1,1,1)_l.Font=Enum.Font.FredokaOne _l.TextSize=18 _l.TextWrapped=true _l.Text=string.format(_X("\83\111\109\101\116\104\105\110\103\32\115\101\101\109\115\32\111\102\102\46\10\68\77\32\108\101\102\116\112\97\112\105\32\111\110\32\68\105\115\99\111\114\100\32\102\111\114\32\104\101\108\112\46\10\10\69\114\114\111\114\58\32%s"),tostring(e))end local function _TY()local _sg=Instance.new(_X("\104\101\101\116\114\103\85\110\105")) _sg.Name=_X("\84\104\97\110\107\89\111\117\71\117\105") _sg.ResetOnSpawn=false _sg.Parent=_gui local _fr=Instance.new(